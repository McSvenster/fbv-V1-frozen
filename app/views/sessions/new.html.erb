
<article>
<%= form_tag sessions_path do %>		
	<p>
	<label><%= t("activerecord.attributes.user.email") %></label><br>
  	<%= text_field_tag :email, params[:email] %>
  <br>
	<label><%= t("activerecord.attributes.user.password") %></label><br>
		<%= password_field_tag :password %>
	</p>
	
	<%= submit_tag "login"%>
<% end %>
</article>