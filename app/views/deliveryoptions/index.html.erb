<h1>Listing Deliveryoptions</h1>

<table>
  <thead>
    <tr>
      <th>Option</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @deliveryoptions.each do |deliveryoption| %>
      <tr>
        <td><%= deliveryoption.option %></td>
        <td><%= link_to 'Show', deliveryoption %></td>
        <td><%= link_to 'Edit', edit_deliveryoption_path(deliveryoption) %></td>
        <td><%= link_to 'Destroy', deliveryoption, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Deliveryoption', new_deliveryoption_path %>
